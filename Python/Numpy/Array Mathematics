import numpy as np

N, _ = map(int, input().split())

A = []
B = []

for i in range(N):
    A.append(np.array(list(map(int, input().split()))))
    
for i in range(N):
    B.append(np.array(list(map(int, input().split()))))
    
print(np.add(A, B))
print(np.subtract(A, B))
print(np.multiply(A, B))
print(np.floor_divide(A, B))
print(np.mod(A, B))
print(np.power(A, B))
